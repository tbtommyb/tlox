class JazzArtist {
  init(name, instrument) {
    this.name = name;
    this.instrument = instrument;
  }

  announce() {
    return this.name + " plays the " + this.instrument;
  }
}

var louie = JazzArtist("Louise Armstrong", "trumpet");

var fieldToAmend = "instr" + "ument";
louie[fieldToAmend] = "super trumpet";

print louie["announce"](); // Expected "Louise Armstrong plays the super trumpet"
